colorscheme candycode

set nu rnu

set nohlsearch

set guifont=Consolas:h12:cRUSSIAN

set langmenu=en_US
let $LANG='en_US'
source $VIMRUNTIME/delmenu.vim
source $VIMRUNTIME/menu.vim

set encoding=utf8

set softtabstop=4
set tabstop=4
set shiftwidth=4
set expandtab

set timeoutlen=1000
set ttimeoutlen=0

set autoindent

filetype plugin indent on

set t_Co=256

let g:netrw_winsize=25

let g:netrw_preview=1

set history=100
